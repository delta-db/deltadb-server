{
  "name": "deltadb-server",
  "version": "0.0.1",
  "description": "An offline-first database",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/delta-db/deltadb-server"
  },
  "keywords": [
    "db",
    "deltadb",
    "offline",
    "offline-first",
    "front-end-only"
  ],
  "author": "Geoffrey Cox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delta-db/deltadb-server/issues"
  },
  "bin": {
    "beautify-proj": "./deltadb-server"
  },
  "scripts": {
    "assert-beautified": "beautify-proj -i test -c beautify.json && beautify-proj -i scripts -c beautify.json",
    "beautify": "beautify-proj -i test -o . -c beautify.json && beautify-proj -i scripts -o . -c beautify.json",
    "jshint": "jshint -c .jshintrc *.js test scripts",
    "node-test": "istanbul test --dir coverage ./node_modules/mocha/bin/_mocha test/index.js",
    "node-full-test": "npm run jshint && npm run node-test --coverage",
    "test": "npm run assert-beautified && npm run node-full-test",
    "TODOtest-performance": "istanbul test ./node_modules/mocha/bin/_mocha -- -g 'delta' test/performance.js"
  },
  "dependencies": {
    "bluebird": "^3.0.2",
    "commander": "^2.9.0",
    "deltadb": "0.0.3",
    "deltadb-common-utils": "0.0.2",
    "deltadb-orm-sql": "0.0.1",
    "events": "^1.1.0",
    "express": "^4.13.3",
    "inherits": "^2.0.1"
  },
  "devDependencies": {
    "beautify-proj": "0.0.4",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5"
  }
}
